  "vim.visualModeKeyBindings": [
    {
      "before": [
        "<leader>"
      ],
      "commands": [
        "whichkey.show"
      ]
    },
    {
      "before": [
        ">"
      ],
      "after": [
        ">",
        "g",
        "v"
      ]
    },
    {
      "before": [
        "<"
      ],
      "after": [
        "<",
        "g",
        "v"
      ]
    },
  ],
  "vim.normalModeKeyBindingsNonRecursive": [
    {
      "before": [
        "<leader>"
      ],
      "commands": [
        "whichkey.show"
      ]
    },
    {
      "before": [
        "C-h"
      ],
      "commands": [
        "workbench.view.explorer"
      ]
    },
    // EASYMoTION REMAP
    {
      "before": [
        "s",
        "s"
      ],
      "after": [
        "<leader>",
        "<leader>",
        "s"
      ]
    },
    {
      "before": [
        "s",
        "f"
      ],
      "after": [
        "<leader>",
        "<leader>",
        "f"
      ]
    },
    {
      "before": [
        "s",
        "F"
      ],
      "after": [
        "<leader>",
        "<leader>",
        "F"
      ]
    },
    {
      "before": [
        "s",
        "w"
      ],
      "after": [
        "<leader>",
        "<leader>",
        "w"
      ]
    },
    {
      "before": [
        "s",
        "b"
      ],
      "after": [
        "<leader>",
        "<leader>",
        "b"
      ]
    },
    {
      "before": [
        "s",
        "e"
      ],
      "after": [
        "<leader>",
        "<leader>",
        "e"
      ]
    },
    {
      "before": [
        "s",
        "j"
      ],
      "after": [
        "<leader>",
        "<leader>",
        "j"
      ]
    },
    {
      "before": [
        "s",
        "k"
      ],
      "after": [
        "<leader>",
        "<leader>",
        "k"
      ]
    },
    // END EASYMOTION
    {
      "before": [
        "S-k"
      ],
      "commands": [
        "editor.action.showHover"
      ]
    },
    // fix bug undo redo
    {
      "before": [
        "u"
      ],
      "after": [],
      "commands": [
        {
          "command": "undo",
          "args": []
        }
      ]
    },
    {
      "before": [
        "<C-r>"
      ],
      "after": [],
      "commands": [
        {
          "command": "redo",
          "args": []
        }
      ]
    },
    // Enable move cursor to each wrapped line (if wrap line enable)
    {
      "before": [
        "j"
      ],
      "after": [
        "g",
        "j"
      ]
    },
    {
      "before": [
        "k"
      ],
      "after": [
        "g",
        "k"
      ]
    }
  ],
  "vim.handleKeys": {
    // "<C-a>": false,
    "<C-f>": false,
    // "<C-d>": false,
    // Copy
    "<C-c>": false,
    // Cut
    // "<C-x>": false,
    // Paste
    // "<C-v>": false,
    // "<C-w>": false,
    "<C-z>": false,
    "<C-y>": false
  },
  // END VIM
  // WhickKey
  "whichkey.sortOrder": "alphabetically",
  "whichkey.delay": 0,
  "whichkey.bindings": [
