  // vim
  "vim.easymotion": true,
  "vim.incsearch": true,
  "vim.useCtrlKeys": true,
  "vim.hlsearch": true,
  "vim.sneak": true,
  // menggunakan clipboard untuk copy
  "vim.useSystemClipboard": true,
  "vim.leader": "<space>",
